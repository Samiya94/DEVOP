//Maven
<properties>
  <maven.compiler.source>8</maven.compiler.source>
  <maven.compiler.target>8</maven.compiler.target>
</properties>

</dependencies>
<build>
  <plugins>
    <plugin>
    <groupId>org.codehause.mojo</groupId>    
    <artifactId>exec-maven-plugin</artifactId>
    <version>3.0.0</version>
    <executions>
      <execution>
      <goals>
        <goal>java</goal>
      </goals>
      </execution>
    </executions>
    <configuration>
      <mainClass>com.example.App</mainClass>
    </configuration>   
</plugin>
</plugins>
</build>
</project>

mvn -version
sudo apt update
sudo apt install maven
mvn archetype:generate -DgroupId=com.example -DartifactId=MyApp -DinterativeMode=false
cd MyApp
tree
sudo apt install tree
mvn compile
mvn exec:java
mvn package

//Jenkins
ngrok http 8080
/github-webhook/
sudo nautilus
sudo chown -R jenkins:jenkins /var/www/html/folder_name
cp -r * /var/www/html/folder_name/

//Docker
FROM httpd:2.4
COPY index.html/usr/local/apache2/htdocs/
sudo docker build -t folder_name
sudo docker run -p 8080:80 -d folder_name
localhost:8080

//Git
mkdir my
cd my
git init
git config --global user.name "sam"
git config --global user.email "sam23@gmail.com"
git config --list
touch index.html
git add index.html
git status
git diff
git commit -m "s1"
git log
touch index.html
git add index.html
git log
